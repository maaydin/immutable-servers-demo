node {
   stage('Checkout') {
      checkout scm
   }
   stage('Copy Artifacts') {
      step ([$class: 'CopyArtifact',
          projectName: 'Build FrontEnd',
          filter: 'frontend/target/*.zip']);
      step ([$class: 'CopyArtifact',
          projectName: 'Build Backend',
          filter: 'backend/target/*.jar']);
   }
}